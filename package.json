{
  "name": "@h1deya/langchain-google-genai-ex",
  "version": "0.1.0",
  "description": "Drop-in replacement for ChatGoogleGenerativeAI that automatically fixes schema compatibility issues with MCP tools and Gemini API",
  "keywords": [
    "langchain",
    "google",
    "gemini",
    "GoogleGenerativeAI",
    "model context protocol",
    "mcp",
    "tools",
    "function calling",
    "schema",
    "varidation",
    "error",
    "400 Error",
    "llm",
    "typescript"
  ],
  "author": "Hideya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hideya/langchain-google-genai-ex.git"
  },
  "bugs": {
    "url": "https://github.com/hideya/langchain-google-genai-ex/issues"
  },
  "homepage": "https://github.com/hideya/langchain-google-genai-ex#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./schema-adapter": {
      "types": "./dist/schema-adapter/index.d.ts",
      "import": "./dist/schema-adapter/index.js",
      "require": "./dist/schema-adapter/index.cjs"
    },
    "./chat-models": {
      "types": "./dist/chat-models/index.d.ts",
      "import": "./dist/chat-models/index.js",
      "require": "./dist/chat-models/index.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/schema-adapter/index.ts src/chat-models/index.ts --format cjs,esm --dts --clean",
    "build:watch": "tsup src/index.ts src/schema-adapter/index.ts src/chat-models/index.ts --format cjs,esm --dts --watch",
    "dev": "tsup src/index.ts src/schema-adapter/index.ts src/chat-models/index.ts --format cjs,esm --dts --watch",
    "test": "node --test dist/test/*.js",
    "test:simple": "tsx src/test/simple.test.ts",
    "test:individual-servers": "tsx src/test/individual-servers.test.ts",
    "test:integration": "tsx src/test/integration.test.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "clean:all": "git clean -fdxn -e .env && read -p '\nOK? ' && git clean -fdx -e .env",
    "publish:test": "npm run clean:all && npm install && npm run build && npm publish --access=public --dry-run",
    "publish:do": "npm run clean:all && npm install && npm run build && npm publish --access=public",
    "docs:build": "npx typedoc --options typedoc.json",
    "docs:deploy": "npm run docs:build && ghp-import -n -p -f docs",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@langchain/google-genai": ">=0.2.16"
  },
  "devDependencies": {
    "@langchain/mcp-adapters": "^1.1.2",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "dotenv": "^16.4.0",
    "eslint": "^9.0.0",
    "langchain": "^1.2.18",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typedoc": "^0.26.0",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
